{
  "name": "@webstudio-is/cli",
  "version": "0.4.0",
  "description": "Webstudio CLI",
  "author": "Webstudio <github@webstudio.is>",
  "repository": "git@github.com:webstudio-is/webstudio-cli.git",
  "homepage": "https://webstudio.is",
  "scripts": {
    "dev": "pnpm build:templates &&  pnpm build --watch",
    "build:templates": "tsx ./templates/build.ts",
    "prebuild": "tsx ./lib/prebuild.js",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build": "pnpm format && pnpm clean && esbuild ./src/* --outdir=./lib --platform=node",
    "clean": "rm -fr ./lib"
  },
  "type": "module",
  "bin": {
    "webstudio": "./bin/cli.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "esbuild": "^0.18.5",
    "prettier": "^3.0.0",
    "tsx": "^3.12.7"
  },
  "dependencies": {
    "@webstudio-is/asset-uploader": "^0.75.0",
    "@webstudio-is/eslint-config-custom": "^1.0.6",
    "@webstudio-is/project-build": "^0.75.0",
    "@webstudio-is/react-sdk": "^0.75.0",
    "@webstudio-is/sdk-components-react": "^0.75.0",
    "@webstudio-is/sdk-components-react-remix": "^0.75.0",
    "@webstudio-is/tsconfig": "^1.0.6",
    "deepmerge-ts": "^5.1.0",
    "xdg-app-paths": "^8.3.0",
    "zx": "^7.2.2"
  }
}
